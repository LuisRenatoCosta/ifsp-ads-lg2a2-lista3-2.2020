package lista3;

public class Mecanica extends Pessoa {
    
    //Atributos
    private String especialidade;
    
    //Método Construtor
    public Mecanica(String nome, String cpf, String especialidade) {
        super(nome, cpf);
        this.especialidade = especialidade;}
    
    //Métodos Getters e Setters
    public String getEspecialidade() {
        return especialidade;}
    public void setEspecialidade(String especialidade) {
        this.especialidade = especialidade;}
    
    //Método toString
    @Override
    public String toString() {
        return "Mecanica{" + "especialidade=" + especialidade + '}';}
    
}
